  object qryProdutosVenda: TZQuery
    Connection = frmPrincipal.conexaoBD
    SQL.Strings = (
      
        'SELECT VP.ID, P.NOME,  VP.QUANTIDADE, VP.VALOR_PROD '#10'FROM VENDAS' +
        '_PROD VP'#10'JOIN PRODUTOS P ON VP.PRODUTO_ID = P.ID'#10'WHERE VP.VENDA_' +
        'ID = 1;')
    Params = <>
    Left = 865
    Top = 363
  end
